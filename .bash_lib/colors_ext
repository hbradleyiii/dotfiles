    printf "${_black_on_grey}%s${_colorreset}\n" "This is _black_on_grey"
    printf "${_blackU_on_grey}%s${_colorreset}\n" "This is _blackU_on_grey"
    printf "${_red_on_grey}%s${_colorreset}\n" "This is _red_on_grey"
    printf "${_redB_on_grey}%s${_colorreset}\n" "This is _redB_on_grey"
    printf "${_redU_on_grey}%s${_colorreset}\n" "This is _redU_on_grey"
    printf "${_green_on_grey}%s${_colorreset}\n" "This is _green_on_grey"
    printf "${_greenB_on_grey}%s${_colorreset}\n" "This is _greenB_on_grey"
    printf "${_greenU_on_grey}%s${_colorreset}\n" "This is _greenU_on_grey"
    printf "${_brown_on_grey}%s${_colorreset}\n" "This is _brown_on_grey"
    printf "${_yellow_on_grey}%s${_colorreset}\n" "This is _yellow_on_grey"
    printf "${_yellowU_on_grey}%s${_colorreset}\n" "This is _yellowU_on_grey"
    printf "${_blue_on_grey}%s${_colorreset}\n" "This is _blue_on_grey"
    printf "${_blueB_on_grey}%s${_colorreset}\n" "This is _blueB_on_grey"
    printf "${_blueU_on_grey}%s${_colorreset}\n" "This is _blueU_on_grey"
    printf "${_magenta_on_grey}%s${_colorreset}\n" "This is _magenta_on_grey"
    printf "${_magentaB_on_grey}%s${_colorreset}\n" "This is _magentaB_on_grey"
    printf "${_magentaU_on_grey}%s${_colorreset}\n" "This is _magentaU_on_grey"
    printf "${_cyan_on_grey}%s${_colorreset}\n" "This is _cyan_on_grey"
    printf "${_cyanB_on_grey}%s${_colorreset}\n" "This is _cyanB_on_grey"
    printf "${_cyanU_on_grey}%s${_colorreset}\n" "This is _cyanU_on_grey"
    printf "${_grey_on_grey}%s${_colorreset}\n" "This is _grey_on_grey"
    printf "${_white_on_grey}%s${_colorreset}\n" "This is _white_on_grey"
    printf "${_whiteU_on_grey}%s${_colorreset}\n" "This is _whiteU_on_grey"
}
# }}}

fi

_COLORS_EXT_DEFINED=true

    # Run testcolors function if called directly
if [[ $0 = */colors_ext ]] ; then
    testextcolors
fi

