# testextcolors () {{{1
#   testextcolors prints a line in each color
function testextcolors() {
    printf "${_dark_grey_on_red}%s${_colorreset}\n" "This is _dark_grey_on_red"
    printf "${_black_on_red}%s${_colorreset}\n" "This is _black_on_red"
    printf "${_blackU_on_red}%s${_colorreset}\n" "This is _blackU_on_red"
